{
    "feats": [
        "Atom",
        "AtomPosition",
        "AtomDistance",
        "CouplingType",
        "RdkitDescriptors",
        "AtomEnvironment",
        "AtomNeighbors",
        "BruteForce",
        "CoulombInteraction",
        "DistanceFromClosest"
